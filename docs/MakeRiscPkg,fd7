IfThere $.Temp Then Else %CDir $.Temp

Set pd$Disc $.Temp.PDManuals_20150218
| Remember to update RiscPkg.Control too and check it in first!

X %Wipe <pd$Disc> ~cfr~v

time
echo Starting svn export - please be patient
svn export <Obey$Dir> <pd$Disc>
time

settype <pd$Disc>.Manuals.PipeDream.RefGuide.index/html FAF

settype <pd$Disc>.Manuals.PipeDream.Tutorial.index/html FAF

| The one in RiscPkg is the useful one for the packaged docs
delete <pd$Disc>.LICENSE
delete <pd$Disc>.MakeRiscPkg

Filer_OpenDir <pd$Disc>
